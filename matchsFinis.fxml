<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="matchsFinisRoot" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MatchsFinis">
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="52.0" text="Id" />
            <TableColumn fx:id="sportMatch" prefWidth="81.0" text="Sport" />
            <TableColumn fx:id="eq1Match" prefWidth="98.0" text="Equipe 1" />
            <TableColumn fx:id="eq2Match" minWidth="0.0" prefWidth="93.0" text="Equipe 2" />
            <TableColumn fx:id="dateMatch" maxWidth="279.0" prefWidth="178.0" text="Date" />
            <TableColumn fx:id="resultat" prefWidth="97.0" text="Résultat" />
         </columns>
      </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="75.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="res" promptText="Résultat du match selectionné" />
                  <Button mnemonicParsing="false" onAction="#saisirResultat" prefHeight="26.0" prefWidth="616.0" text="Saisir" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
